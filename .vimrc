syntax enable
set number
set nowrap
set modeline
set ls=2
set clipboard=unnamedplus
set background=dark
let g:solarized_degrade=1
let g:solarized_termcolors=256
set tabstop=2
set shiftwidth=2
set smartindent
set autoindent
filetype indent on
set smarttab
set cindent
set expandtab
colorscheme solarized
